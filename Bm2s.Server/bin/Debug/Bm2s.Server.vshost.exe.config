<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="DbProvider" value="PostgreSql"/>
    <add key="ListeningIp" value="127.0.0.1"/>
    <add key="ListeningPort" value="8090"/>
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Npgsql" publicKeyToken="5d8b90d52f46fda7" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.2.5.0" newVersion="2.2.5.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <connectionStrings>
    <add name="bm2s" providerName="NpgSql" connectionString="Server=127.0.0.1;Port=5432;Database=bm2s;User Id=bm2s;Password=bm2s;"/>
  </connectionStrings>
  <system.data>
    <DbProviderFactories>
      <add name="PostgreSQL Data Provider"
         invariant="Npgsql"
         description=".Net Framework Data Provider for PostgreSQL"
         type="Npgsql.NpgsqlFactory, Npgsql, Version=4.0.0.0, Culture=neutral, PublicKeyToken=5d8b90d52f46fda7"/>
    </DbProviderFactories>
  </system.data>
</configuration>